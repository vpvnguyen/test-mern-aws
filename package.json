{
  "name": "project3-backend-prototype",
  "version": "1.0.0",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "formik": "^1.5.8",
    "if-env": "^1.0.4",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "material-table": "^1.52.0",
    "node-geocoder": "^3.24.0",
